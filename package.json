{
  "name": "pillar.gg",
  "version": "1.0.0",
  "private": true,
  "description": "The pillar.gg website.",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "vercel dev --listen 8000",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-descriptions": "^1.9.21",
    "@ant-design/pro-form": "^1.43.1",
    "@ant-design/pro-layout": "^6.26.2",
    "@ant-design/pro-table": "^2.55.0",
    "@aws-sdk/client-sfn": "^3.39.0",
    "@aws-sdk/client-sns": "^3.36.0",
    "@babel/runtime": "^7.15.4",
    "@dnd-kit/sortable": "^2.0.1",
    "@hubspot/api-client": "^3.4.1",
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.19.1",
    "@umijs/route-utils": "^1.0.37",
    "@vercel/node": "^1.12.1",
    "antd": "4.17.0-alpha.7",
    "classnames": "^2.3.1",
    "d3-array": "^2.8.0",
    "date-fns": "^2.25.0",
    "enquire-js": "^0.2.1",
    "googleapis": "^84.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "mongodb": "^3.6.2",
    "node-fetch": "^2.6.1",
    "omit.js": "^2.0.2",
    "qs": "^6.9.0",
    "rc-banner-anim": "^2.4.4",
    "rc-queue-anim": "^1.8.5",
    "rc-scroll-anim": "^2.7.5",
    "rc-tween-one": "^2.7.3",
    "react": "^16.8.6",
    "react-compound-slider": "^3.3.1",
    "react-cropper": "^2.1.8",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.1.2",
    "react-player": "^2.9.0",
    "react-sublime-video": "^0.2.5",
    "stripe": "^8.181.0",
    "styled-components": "^5.3.1",
    "swr": "1.0.1",
    "twitch": "^4.6.7",
    "twitch-webhooks": "^4.6.7",
    "umi": "^3.5.20",
    "umi-request": "^1.4.0",
    "use-merge-value": "^1.0.1",
    "uuid": "^8.3.2",
    "vercel": "^23.1.2",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.175",
    "@types/qs": "^6.9.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^6.1.3",
    "@types/styled-components": "^5.1.14",
    "@umijs/fabric": "^2.2.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.0-beta.2",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.24",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.4.1",
    "pro-download": "1.0.1",
    "puppeteer-core": "^5.0.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "packageManager": "yarn@3.1.0"
}
